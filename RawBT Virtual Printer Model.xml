<?xml version="1.0" encoding="UTF-8"?>
<printer-models xmlns="http://www.jeremias-maerki.ch/ns/print">
  
  <!--
    
    This is just simulating some arbitrary ESC/POS printer on the RawBT mobile app that can server as virtual printer and testbed.
    
  -->
  
  <printer-model>
    <model>RawBT Virtual Printer</model>
    <driver>ESC/POS</driver>
    <escpos xmlns="http://www.jeremias-maerki.ch/ns/print/escpos">
      <resolution feed="203" cross-feed="203"/>
      <width max="720px"/> <!-- 62mm + 2*12px margin -->
      <length min="150px" max="11811px"/> <!-- 12.7mm to 1000mm -->
    </escpos>
  </printer-model>
  
</printer-models>
